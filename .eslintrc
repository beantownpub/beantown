{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 0,
        "no-unused-vars": 0,
        "no-console": "off",
        "arrow-body-style": ["as-needed"]
    },
    "extends": "eslint:recommended"
}
